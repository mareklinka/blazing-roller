{
    "compilerOptions": {
      "baseUrl": "./",
      "outDir": "./dist/out-tsc",
      "sourceMap": true,
      "declaration": false,
      "module": "es6",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "target": "es6",
      "typeRoots": ["node_modules/@types"],
      "lib": ["es2018", "dom"],
      "strictNullChecks": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "suppressImplicitAnyIndexErrors": true,
      "strict": true
    }
  }
